{
  "name": "console-app",
  "version": "1.0.0",
  "scripts": {
    "start": "npx -y ai-install --ignore-scripts puppeteer && npm run prepare && ng serve",
    "prepare": "mkdir dist -p && pegjs src/app/components/graph/dotParser.pegjs dist/dotParser.js",
    "build": "npx -y ai-install --ignore-scripts puppeteer && npm run prepare && ng build --configuration production",
    "serve": "npx -y ai-install --ignore-scripts puppeteer && ng serve --configuration development",
    "test": "npx -y ai-install --ignore-scripts puppeteer && ng test"
  },
